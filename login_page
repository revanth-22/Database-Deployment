	function writePwd(s){
		
		if( ((document.frmLogin.password[1].value).length >= 0 ) && ((document.frmLogin.password[1].value).length <= 15) ) {
			document.frmLogin.password[1].value=document.frmLogin.password[1].value+s;			
			testPassword3();
			
		}else{
			return false;
		}
	}

