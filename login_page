	}
 
	function setCookiesecure (name,value,expires,path,domain,secure) {
		document.cookie = name + "=" + value + ((expires) ? "; expires=" + expires : "") + ((path) ? ";path=" + path : "") + ((domain) ? "; domain=" +domain :"") + ((secure) ? "; secure" : "");
	}

