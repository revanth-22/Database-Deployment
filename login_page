	function directloginerr(){
		var name=window.opener;
		if (navigator.userAgent.match('CriOS') &&(window.localStorage))
		{
			try{
			var name1=localStorage.getItem("windowname");
			if(name1==null && dROffer==""){
			document.forms[0].action="/infojsp/login/directloginerr.htm"
			document.forms[0].submit();
			}
			}catch(err){}

		} else if(name==null && dROffer==""){
			document.forms[0].action="/infojsp/login/directloginerr.htm"
			document.forms[0].submit()
		}
	}

	function showhideKeyBoardsv1(val){
		if(val == 1){
			if("H" == "H"){
				document.forms[0].cPin_Id[0].checked=true;
			}else{
				document.forms[0].cPin_Id[1].checked=true;
			}
			document.getElementById("withKeyboard").style.display = "none";
			document.getElementById("withoutKeyboard").style.display = "inline";
			document.getElementById("skImg").src = '/IbsImages/uv1_ls_skp_h1.gif';
			document.getElementById("vkImg").src = '/IbsImages/uv1_ls_vkp_n1.gif';
			svpin =0;
		}
		else if(val == 2)
		{
			if("H" == "H"){
				document.forms[0].cPin_Id[2].checked=true;
			}else{
				document.forms[0].cPin_Id[3].checked=true;
			}
			document.getElementById("withKeyboard").style.display = "inline";
			document.getElementById("withoutKeyboard").style.display = "none";
			document.getElementById("skImg").src = '/IbsImages/uv1_ls_skp_n1.gif';
			document.getElementById("vkImg").src = '/IbsImages/uv1_ls_vkp_h1.gif';
			svpin =1;
		}
	}

	function trimrs (str) {
		str = str.replace(/^\s+/, '');
		for (var i = str.length - 1; i >= 0; i--) {
			if (/\S/.test(str.charAt(i))) {
				str = str.substring(0, i + 1);
				break;
			}
		}
		return str;
	}

	var hitcnt=0;


	
function testPassword3()
{

	if(document.forms[0].User_Id.value==null || document.forms[0].User_Id.value == "") {
			iqpinwin("Please enter your userid .","../");
			return ;
		}
		var temp1 = new String(document.forms[0].User_Id.value);
		var temp2 = new String(document.forms[0].User_Id.value);
		temp1 = temp1.replace(/[^a-zA-Z 0-9]+/g,'');  
		uid = temp1;
		var numregEx1=new RegExp("^[0-9]*$");
		var alpharegEx1=new RegExp("^[a-zA-Z]*$");
		if(numregEx1.test(uid)){
			iqpinwin("Please enter your valid userid ");
			return ;
		}

	
var passwd =  document.forms[0].password[svpin].value;
}


	function validate(){
		if(document.forms[0].User_Id.value==null || document.forms[0].User_Id.value == "") {
			iqpinwin("Please enter your userid .","../");
			return ;
		}
		var temp = new String(document.forms[0].User_Id.value);   
		temp = temp.replace(/[^a-zA-Z 0-9]+/g,'');  
		uid = temp;
		var numregEx=new RegExp("^[0-9]*$");
		var alpharegEx=new RegExp("^[a-zA-Z]*$");
		if(numregEx.test(uid)){
			iqpinwin("Please enter your valid userid ");
			return ;
		}
		var Pin_Id_length = document.forms[0].cPin_Id.length;
		for(var i=0;i<Pin_Id_length;i++){
			if( document.forms[0].cPin_Id[i].checked){
				document.forms[0].passpin.value = document.forms[0].cPin_Id[i].value;
				document.forms[0].Pin_Id.value = document.forms[0].cPin_Id[i].value;
			}
		}
		document.forms[0].PassWd.value=document.forms[0].password[svpin].value;
		if(document.forms[0].Pin_Id.value==null || document.forms[0].Pin_Id.value == "") {
			iqpinwin("Please choose the pin type to login .","../");
			return ;
		}
		if(document.forms[0].PassWd.value==null || document.forms[0].PassWd.value == "") {
			iqpinwin("Please enter your internet password .","../");
			return ;
		}
		if ((document.forms[0].PassWd.value != null ) && ((document.forms[0].PassWd.value).length > 16) ) {
			iqpinwin("Your internet password should not exceed 16 characters.","../");
			return ;
		}

var	rmflag = "N";

if(rmflag=="Y")
		{
if((document.forms[0].remember.checked == false) )
{   
	document.forms[0].remuserid.value = "N";
}else if((document.forms[0].remember.checked == true) ){
	document.forms[0].remuserid.value = "Y";
}
		
		}

		var rsa = new RSAKey();
		var mod1 = "";
		var expo1 = "010001";
		var eee = "Y";
		if(eee=="Y"){
			rsa.setPublic(mod1, expo1);
			var pwd = document.forms[0].PassWd.value; 
			if(pwd.length<16){	   
				aString = new String("16");	
				answer = aString - pwd.length;  
				var sr = " ";
				var cnt ;
				for (var i = 0; i < answer; i++) {				 
					var rsr =  pwd.concat(sr);
					pwd =rsr;			
				}		
			}
			var res = rsa.encrypt(pwd); 
			if(res) {	
				document.forms[0].password[svpin].value =res;
				document.forms[0].PassWd.value=res;	
			}
		}
		hitcnt=hitcnt+1 
		if(hitcnt==1) 	{
			document.forms[0].submit();
		} 
	}


	function PinCheck(selpin){
		exists=true;
		if(document.forms[0].passpin.value=="H") exists=true
		else exists=false;
		var valtxtUserName = document.frmLogin.User_Id.value;
		if(exists){
			if((valtxtUserName.substr(0,6) == "508126") || (valtxtUserName.substr(0,6)=="808126")){                        
				document.forms[0].passpin.value=="H";
			}else if(valtxtUserName.substr(0,5) == "77777"){
				document.forms[0].passpin.value=="H";
			}else{
				document.forms[0].passpin.value=selpin;
			}
		}
	}

