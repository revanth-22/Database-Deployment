	}

	function convertToUpperCase()
	{
		var usid=document.frmLogin.User_Id.value;         
		document.frmLogin.User_Id.value=usid.toUpperCase();
	}

	  var toggle = function() {
  var mydiv = document.getElementById('dhtmltooltip_n');
  if (mydiv.style.display === 'block' || mydiv.style.display === '')
    mydiv.style.display = 'none';
  else
    mydiv.style.display = 'block'
  }


	  var toggle1 = function() {
