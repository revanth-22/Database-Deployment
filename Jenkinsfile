   agent docker-latest
   stages {
      stage('start') {
               
        steps { echo 'pipeline has started'
               echo '********************'}
      }

      stage('git clone') {
               echo 'pipeline has started'
               echo '********************'
      }

      stage('test cases') {
               echo 'pipeline has started'
               echo '********************'
      }

      stage('test cases publish') {
               echo 'pipeline has started'
               echo '********************'
      }

      stage('Deploy on Dev environment ') {
               echo 'pipeline has started'
               echo '********************'
      }

      stage('Deploy on Prod environment') {
               echo 'pipeline has started'
               echo '********************'
      }
}

}
